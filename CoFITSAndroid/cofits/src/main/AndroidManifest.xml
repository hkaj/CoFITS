<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.android.cofits" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.android.cofits.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.android.cofits.ProjectsActivity"
            android:label="@string/title_activity_project" >
        </activity>
        <activity
            android:name="com.android.cofits.SessionsActivity"
            android:label="@string/title_activity_session" >
        </activity>
        <activity
            android:name="com.android.cofits.FilesActivity"
            android:label="@string/title_activity_files" >
        </activity>
        <activity
            android:name="com.android.cofits.AddFileActivity"
            android:label="@string/title_activity_add_file" >
        </activity>
        <activity
            android:name="com.android.cofits.FileViewActivity"
            android:label="@string/title_activity_file_view" >
        </activity>
        <activity
            android:name="com.android.cofits.ListFileActivity"
            android:label="@string/title_activity_list_file" >
        </activity>
        <activity
            android:name="com.android.cofits.FileExplore"
            android:label="@string/title_activity_file_explore" >
        </activity>
    </application>

</manifest>
